{
  "name": "gitbook-plugin-swagger-storm",
  "version": "0.2.0",
  "description": "A plugin to consume Swagger files into a Gitbook for storm-online",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kordeviant/gitbook-plugin-swagger.git"
  },
  "keywords": [
    "gitbook",
    "plugin"
  ],
  "author": "Puria Kordrostami<kordeviant@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kordeviant/gitbook-plugin-swagger#readme",
  "engines": {
    "gitbook": ">=3"
  },
  "gitbook": {
    "properties": {
      "file": {
        "type": "string",
        "default": "",
        "description": "Relative path to snippets file to use."
      }
    }
  },
  "dependencies": {
    "bootprint": "^1.0.0",
    "bootprint-swagger": "^1.0.1"
  }
}
