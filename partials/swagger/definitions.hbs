{{!--
Renders the definition-section of the HTML-page.
@param {Definition[]} definitions a list of JSON-subschemas.
@api public
--}}

{{#if definitions}}
<h2>Schema definitions</h2>

{{#eachSorted definitions}}
<strong>{{@key}} :</strong>
<table class="table">
    <thead>
        <tr>
            <th class="sw-param-property">Property</th>
            <th class="sw-param-Type">Type</th>
        </tr>
    </thead>
    <tbody>
        {{#each properties}}
        <tr>
            <td>{{@key}}</td>
            <td>
                {{this.type}} 
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>

{{/eachSorted}}
{{/if}}